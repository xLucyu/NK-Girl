create table if not exists GUILDS (
  GuildID TEXT PRIMARY KEY,
  RaceChannelID TEXT,
  BossChannelID TEXT,
  OdysseyChannelID TEXT,
  CollectionChannelID TEXT,
  Raceids TEXT,
  Bossids TEXT,
  Odysseyids TEXT,
  Collectionids TEXT
);


create table if not exists USAGE (
  Command TEXT PRIMARY KEY,
  Uses INTEGER DEFAULT 0
);
